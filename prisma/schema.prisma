datasource db {
    provider =  "postgresql"
    url = env("DATABASE_URL")
}

generator db {
    provider = "prisma-client-py"
    interface = "asyncio"
    recursive_type_depth = 5
}

model User {
    id String @id @default(cuid())
    firstName String?
    lastName String?
    email String @unique
    password String
    image String?
    links String[] @default([])
    created_at DateTime @default(now())
    updated_at DateTime @updatedAt
}

model Platform {
    id String @id @default(cuid())
    name String
    lightIcon String
    darkIcon String
    previewColor String
}